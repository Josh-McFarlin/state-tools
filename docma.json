{
  "debug": 5,
  "jsdoc": {
    "encoding": "utf8",
    "recurse": false,
    "pedantic": false,
    "access": null,
    "package": null,
    "module": true,
    "undocumented": false,
    "undescribed": false,
    "ignored": false,
    "hierarchy": true,
    "sort": "alphabetic",
    "relativePath": null,
    "filter": null,
    "allowUnknownTags": true,
    "plugins": []
  },
  "markdown": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "sanitize": false,
    "smartLists": true,
    "smartypants": false,
    "xhtml": false,
    "tasks": true,
    "emoji": true
  },
  "app": {
    "title": "state-tools",
    "meta": null,
    "base": "/state-tools",
    "entrance": "content:readme",
    "routing": "path",
    "server": "github"
  },
  "template": {
    "path": "zebra",
    "options": {
      "title": {
        "label": "state-tools",
        "href": "."
      },
      "sidebar": {
        "enabled": true,
        "outline": "tree",          // "flat" | "tree"
        "collapsed": false,
        "toolbar": true,
        "itemsFolded": false,
        "itemsOverflow": "crop",    // "crop" | "shrink"
        "badges": true,             // true | false | <string-value>
        "search": true,
        "animations": true
      },
      "symbols": {
        "autoLink": true,           // "internal" | "external" | true (both)
        "params": "list",           // "list" | "table"
        "enums": "list",            // "list" | "table"
        "props": "list",            // "list" | "table"
        "meta": false
      },
      "contentView": {
        "bookmarks": "h1,h2,h3",
        "faLibs": "all"             // "all" or "solid"|"regular"|"brands" or comma-separated or null
      },
      "navbar": {
        "enabled": true,
        "fixed": true,
        "dark": false,
        "animations": true
      }
    }
  },
  "src": [
    "./src/**/*.js",
    "./README.md"
  ],
  "dest": "./docs",
  "clean": true
}
