{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": true,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "docdash": {
    "search": true,
    "sort": true,
    "navLevel": 1,
    "menu":{
      "GitHub":{
        "href": "https://github.com/Josh-McFarlin/state-tools",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    },
    "meta": {
      "title": "state-tools",
      "description": "A library to help easily modify React state in one line."
    }
  }
}
